#!/bin/bash

$JAVA_HOME/bin/java -server -d64 -XX:+UseCompressedOops -XX:+DoEscapeAnalysis -Xmx2g -Xms2g -Xmn1g -XX:+UseConcMarkSweepGC $JAVA_OPTS -Xbootclasspath/p:$DELITE_HOME/unsafe_classes/unsafe.jar -cp $DELITE_HOME/runtime/target/scala-2.10.0-M1-virtualized/classes:$SCALA_VIRT_HOME/lib/scala-library.jar:$SCALA_VIRT_HOME/lib/scala-compiler.jar:$SCALA_VIRT_HOME/* ppl.delite.runtime.Delite "$@"
